{
    "/proxy/*": {
        "context": "/proxy/*",
        "target": "http://apigestor.tech:443",
        "secure": false,
        "changeOrigin": true,
        "headers": { "Connection": "keep-alive"},
        "pathRewrite": {
            "^/proxy": "http://apigestor.tech:443/api"
          }
    }
}